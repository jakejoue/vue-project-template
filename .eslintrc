{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "prettier/prettier": "error",
        "camelcase": "error",
        "no-var": "error",
        "prefer-const": "error",
        "no-console": "warn",
        "no-debugger": "warn"
    },
    "globals": {
        "envIsDebug": "readonly",
        "ROOTVM": "readonly",
        "CONFIG": "readonly"
    }
}